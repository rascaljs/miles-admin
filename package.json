{
  "name": "miles-admin",
  "version": "0.0.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "commit": "ras git-commit",
    "cleanup": "ras cleanup",
    "format": "ras eslint-prettier",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "release": "ras release",
    "ncu": "ras update-pkg",
    "tsx": "tsx",
    "prepare": "pnpm ras init-git-hooks"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "@rascaljs/vue-materials": "0.1.4"
  },
  "devDependencies": {
    "@unocss/preset-uno": "0.56.0",
    "@unocss/transformer-directives": "0.56.0",
    "@unocss/vite": "0.56.0",
    "lint-staged": "15.0.2",
    "npm-check-updates": "16.14.6",
    "simple-git-hooks": "2.9.0",
    "@rascaljs/cli": "0.4.0",
    "@types/node": "20.8.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-rascal": "0.2.3",
    "naive-ui": "^2.35.0",
    "sass": "^1.69.4",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm ras git-commit-verify",
    "pre-commit": "pnpm lint && ras lint-staged"
  }
}
